{
  "name": "odata-playground-e2e",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/odata-playground-e2e",
  "projectType": "application",
  "targets": {
    "e2e": {
      "executor": "@ns3/nx-playwright:playwright",
      "options": {
        "command": "playwright test",
        "config": "apps/odata-playground-e2e/playwright.config.ts",
        "devServerTarget": "odata-playground:serve:production"
      },
      "configurations": {
        "debug": {
            "command": "playwright test --headed --debug"
        }
      }
    },
    "show-results": {
      "executor": "@ns3/nx-playwright:playwright",
      "options": {
        "command": "playwright show-report dist/apps/odata-playground-e2e/playwright-report"
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/my-desktop-e2e/**/*.{js,ts}"]
      }
    }
  },
  "tags": []
}
